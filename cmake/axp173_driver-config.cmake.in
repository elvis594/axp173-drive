@PACKAGE_INIT@

include(CMakeFindDependencyMacro)

# 检查组件
set(_axp173_driver_supported_components axp173 i2c_manager)

foreach(_comp ${axp173_driver_FIND_COMPONENTS})
    if (NOT _comp IN_LIST _axp173_driver_supported_components)
        set(axp173_driver_FOUND False)
        set(axp173_driver_NOT_FOUND_MESSAGE "Unsupported component: ${_comp}")
    endif()
endforeach()

# 包含目标文件
include("${CMAKE_CURRENT_LIST_DIR}/axp173_driver-targets.cmake")

check_required_components(axp173_driver)